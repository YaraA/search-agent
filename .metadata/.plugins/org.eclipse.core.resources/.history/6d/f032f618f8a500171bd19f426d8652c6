
public class PressurePads extends Cell {
	
	boolean isActivated;
	//Cell rockOnTop;
	
	public PressurePads(int positionRow, int positionCol) {
		super(positionRow, positionCol);
		isActivated= false;
		
	}

	public boolean canMoveThrough(){
		if(isActivated)
			return false;
		else
			return true; 
	}


}
